<div class="container">
 <div class="col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">

   <section class="check">
    <h3>以下の内容で、送信する。</h3>
   </section>
  <%= form_for(@contact) do |f| %>
  <%= f.hidden_field :name %>
  <%= f.hidden_field :email %>
  <%= f.hidden_field :content %>

  <p>name:<%= @contact.name %></p>
  <p>email:<%= @contact.email %></p>
  <p>content:<%= @contact.content %></p>
  <%= f.submit "送信する" %>
   <% end %>

  <%= form_for(@contact, url: new_contact_path, method: 'get') do |f| %>
  <%= f.hidden_field :name %>
  <%= f.hidden_field :email %>
  <%= f.hidden_field :content %>
  <%= f.submit "もどる", name: 'back'  %>
   <% end %>

  </div>
</div>